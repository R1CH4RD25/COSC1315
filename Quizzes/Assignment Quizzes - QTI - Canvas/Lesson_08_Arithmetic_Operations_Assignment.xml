                </material>
              </response_label>
              <response_label ident="c">
                <material>
                  <mattext texttype="text/plain">3.0</mattext>
                </material>
              </response_label>
              <response_label ident="d">
                <material>
                  <mattext texttype="text/plain">1</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
