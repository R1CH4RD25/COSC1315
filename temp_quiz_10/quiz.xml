        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt;What is the Logical AND (and) operator?&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="a">
                <material>
                  <mattext texttype="text/plain">An operator that returns True only if BOTH conditions are true; used when all requirements must be met</mattext>
                </material>
              </response_label>
              <response_label ident="b">
                <material>
                  <mattext texttype="text/plain">An operator that returns True if at least one condition is true</mattext>
                </material>
              </response_label>
              <response_label ident="c">
                <material>
                  <mattext texttype="text/plain">An operator that inverts boolean values</mattext>
                </material>
              </response_label>
              <response_label ident="d">
                <material>
                  <mattext texttype="text/plain">An operator that combines strings together</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">a</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
