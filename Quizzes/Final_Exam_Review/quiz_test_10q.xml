            </qtimetadatafield>
            <qtimetadatafield>
              <fieldlabel>points_possible</fieldlabel>
              <fieldentry>2</fieldentry>
            </qtimetadatafield>
          </qtimetadata>
        </itemmetadata>
        <presentation>
          <material>
            <mattext texttype="text/html">&lt;p&gt;What is the purpose of the &lt;code&gt;print()&lt;/code&gt; function in Python?&lt;/p&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="a">
                <material>
                  <mattext texttype="text/plain">To format text with different fonts</mattext>
                </material>
              </response_label>
              <response_label ident="b">
                <material>
                  <mattext texttype="text/plain">To display output to the screen</mattext>
                </material>
              </response_label>
              <response_label ident="c">
                <material>
                  <mattext texttype="text/plain">To save data to a file</mattext>
                </material>
              </response_label>
              <response_label ident="d">
                <material>
                  <mattext texttype="text/plain">To get input from the user</mattext>
                </material>
              </response_label>
            </render_choice>
          </response_lid>
        </presentation>
        <resprocessing>
          <outcomes>
            <decvar maxvalue="100" minvalue="0" varname="SCORE" vartype="Decimal"/>
          </outcomes>
          <respcondition continue="No">
            <conditionvar>
              <varequal respident="response1">b</varequal>
            </conditionvar>
            <setvar action="Set" varname="SCORE">100</setvar>
          </respcondition>
        </resprocessing>
      </item>


